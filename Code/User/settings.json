{
    "editor.fontFamily": "MesloLGMDZ Nerd Font",
    "workbench.colorTheme": "Material Theme",
    "workbench.iconTheme": "material-icon-theme",
    "materialTheme.accent": "Orange",
    // terminal settings
    "terminal.integrated.fontFamily": "MesloLGMDZ Nerd Font",
    "terminal.integrated.fontSize": 14,
    "terminal.external.osxExec": "iTerm.app",
    "workbench.startupEditor": "newUntitledFile",
    "eslint.autoFixOnSave": true,
    "window.zoomLevel": 0,
    "emmet.showExpandedAbbreviation": "always",
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.leader": ",",
    "vim.useCtrlKeys": true,
    "vim.easymotion": true,
    "vim.statusBarColorControl": false,
    // "vim.statusBarColors.normal": ["#263238", "#ffffff"],
    // "vim.statusBarColors.insert": ["#fb4934", "#ffffff"],
    // "vim.statusBarColors.visual": ["#d65d0e", "#ffffff"],
    // "vim.statusBarColors.visualline": ["#d65d0e", "#ffffff"],
    // "vim.statusBarColors.visualblock": ["#fe8019", "#ffffff"],
    // "vim.statusBarColors.replace": ["#fabd2f", "#ffffff"],
    "vim.visualstar": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before":["<C-n>"],
            "commands": [
                ":nohl",
            ]
        },
        {
            "before": ["leader", "w"],
            "commands": [
                "workbench.action.files.save",
            ]
        },
    ],
    "cSpell.userWords": [
        "CSRF",
        "Callout",
        "Dropzone",
        "RECAPTCHA",
        "Subheader",
        "Truthy",
        "Unfollow",
        "Unmount",
        "allowtransparency",
        "blueimp",
        "chromedriver",
        "commentable",
        "copyhistory",
        "datepicker",
        "datetime",
        "dropdown",
        "embedly",
        "endblock",
        "followee",
        "followees",
        "followership",
        "gaggleamp",
        "glassdoor",
        "influxive",
        "javascripts",
        "keyframes",
        "keyline",
        "linkedin",
        "loadimage",
        "mailchimp",
        "noninteractive",
        "pinterest",
        "playsinline",
        "polyfill",
        "pubsub",
        "quora",
        "resizable",
        "schedulable",
        "scrollable",
        "scroller",
        "selectable",
        "setstate",
        "skusic",
        "slideshare",
        "smiley",
        "stylesheets",
        "turbolinks",
        "tzinfo",
        "unhighlight",
        "unvote",
        "urijs",
        "vkontakte",
        "walkthrough",
        "webm",
        "webp",
        "webpacker",
        "woff",
        "wtforms",
        "xing",
        "youtube"
    ],
    "editor.minimap.enabled": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "editor.lineNumbers": "relative",
    "workbench.sideBar.location": "right",
    "eslint.packageManager": "yarn",
    "breadcrumbs.enabled": true,
    "editor.accessibilitySupport": "off",
    "editor.dragAndDrop": false,
    "window.newWindowDimensions": "fullscreen",
    "window.restoreFullscreen": true,
    "files.exclude": {
        "**/.bundle": true
    },
    "liveshare.audio.startCallOnShare": true,
    "ruby.pathToBundler": "/Users/ku5ic/.rbenv/shims/bundle",
    "diffEditor.ignoreTrimWhitespace": false,
    "solargraph.formatting": true,
    "ruby.interpreter.commandPath": "/Users/ku5ic/.rbenv/shims/ruby",
    "solargraph.commandPath": "/Users/ku5ic/.rbenv/shims/solargraph",
    "terminal.explorerKind": "external",
    "terminal.integrated.rendererType": "auto",
    "solargraph.bundlerPath": "/Users/ku5ic/.rbenv/shims/bundle",
    "workbench.colorCustomizations": {
    //     "statusBar.background": "#d65d0e",
    //     "statusBar.noFolderBackground": "#d65d0e",
    //     "statusBar.debuggingBackground": "#d65d0e",
        "statusBar.foreground": "#ffffff",
        "editorLineNumber.foreground": "#666",
        "editorLineNumber.activeForeground": "#d65d0e"
    },
    "workbench.fontAliasing": "antialiased",
    "workbench.tips.enabled": false,
    "editor.fontLigatures": false,
    "workbench.statusBar.feedback.visible": false,
    "workbench.tree.indent": 12,
    "workbench.editor.enablePreview": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.labelFormat": "default",
    "workbench.settings.editor": "ui",
    "workbench.editor.tabSizing": "shrink",
    "workbench.settings.useSplitJSON": false,
    "editor.fontSize": 14,
    "editor.cursorStyle": "underline",
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "terminal.integrated.cursorStyle": "underline",
    "npm.packageManager": "yarn",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[html]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "liveshare.featureSet": "insiders",
    "python.autoComplete.addBrackets": true,
    "python.autoComplete.showAdvancedMembers": false,
    "python.terminal.activateEnvironment": false,
    "editor.tabSize": 2,
    "[python]": {
        "editor.tabSize": 4
    },
    "material-icon-theme.folders.theme": "specific"
}
