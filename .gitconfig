[push]
  default = current
[user]
  name = Sinisa Kusic
  email = contact.sinisa@gmail.com
[core]
  editor = vim
  whitespace = trailing-space,space-before-tab
  excludesfile = /Users/ku5ic/.gitignore_global
  ignorecase = false
[color]
  ui = auto
  interactive = true
  status = true
  branch = true
  diff = true
[commit]
  template = /Users/ku5ic/.gitmessage
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[merge]
	tool = vimdiff
	conflictstyle = diff3
[mergetool]
	prompt = false
	keepBackup = false
