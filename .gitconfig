[push]
	default = current
[user]
	name = Sinisa Kusic
	email = contact.sinisa@gmail.com
	signingkey = ssh-rsa 
[core]
	editor = nvim
	whitespace = trailing-space,space-before-tab
	excludesfile = /Users/ku5ic/.gitignore_global
	ignorecase = false
[color]
	ui = auto
	interactive = true
	status = true
	branch = true
	diff = true
[commit]
	template = /Users/ku5ic/.gitmessage
	gpgsign = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[merge]
	tool = nvimdiff
[mergetool]
	prompt = false
	keepBackup = false
[diff]
	tool = nvimdiff
[gpg]
	format = ssh
[gpg "ssh"]
	program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
