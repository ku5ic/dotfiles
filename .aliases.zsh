# git
alias gp="git pull"
alias gc="git commit"
alias ga="git add"
alias gpo="git push origin"
alias gb="git branch"
alias gcb="git checkout -b"
alias gr="git rebase"

alias bat="bat --style=numbers --color=always --theme=TwoDark"

# tmuxinator
alias mux=tmuxinator

alias ll="ls -la"

# brew
alias brewall="brew update; brew upgrade; brew upgrade --cask; brew cleanup --prune=all; brew autoremove; brew doctor;"


# general
alias c="clear"
alias h="history"
alias j="jobs"
alias k="kill"
